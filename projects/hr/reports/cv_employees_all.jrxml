	<queryString>
		<![CDATA[SELECT vw_employees.employee_name, vw_employees.date_of_birth, vw_employees.gender_name, vw_employees.language,
	vw_employees.marital_status_name, vw_employees.sys_country_name,
	vw_employees.objective, vw_employees.interests,
	address.post_office_box, address.postal_code, address.town, address.mobile, address.email
FROM  vw_employees LEFT JOIN address ON vw_employees.entity_id = address.table_id
WHERE ((address.table_name is null) OR (address.table_name = 'employees')) AND (vw_employees.active = true)
$P!{organd};]]>
