	<queryString>
		<![CDATA[SELECT vw_employee_month.entity_id, vw_employee_month.entity_name, vw_employee_month.period_year,
	vw_employee_month.period_month,
	vw_employee_month.exchange_rate * (vw_employee_month.basic_pay +
	 vw_employee_month.tax_allowance + getAdjustment(vw_employee_month.employee_month_id, 4, 32))
	as grosspay,
	Employee_Tax_Types.Amount as calcpaye,
	vw_employee_month.exchange_rate *
	(getAdjustment(vw_employee_month.employee_month_id, 4, 26) + vw_employee_month.tax_tax) as calcnssf,

	(vw_employee_month.exchange_rate *
	getAdjustment(vw_employee_month.employee_month_id, 1, 27)) as owner_occupier,

	(vw_employee_month.exchange_rate *
	getAdjustment(vw_employee_month.employee_month_id, 2, 28)) as insurance_relief,

	Period_Tax_Types.tax_relief,
	(Employee_Tax_Types.Amount + Period_Tax_Types.tax_relief) as fulltax,
	Employee_Tax_Types.tax_identification as pinnumber,
	vw_employee_month.exchange_rate * (vw_employee_month.basic_pay +
		vw_employee_month.tax_allowance - vw_employee_month.tax_tax) as chargeable,
	(vw_employee_month.exchange_rate * vw_employee_month.basic_pay * 0.3) as e1,
	cast(20000 as real) as MaxContribRelief,
	orgs.org_name, orgs.pin

FROM orgs, (vw_employee_month INNER JOIN Employee_Tax_Types ON vw_employee_month.Employee_Month_ID = Employee_Tax_Types.Employee_Month_ID
	INNER JOIN Period_Tax_Types ON (vw_employee_month.Period_ID = Period_Tax_Types.Period_ID)
	AND (Employee_Tax_Types.Tax_Type_ID = Period_Tax_Types.Tax_Type_ID))

WHERE ((Period_Tax_Types.Tax_Type_ID = 1) OR (Period_Tax_Types.Tax_Type_ID = 4))
AND (vw_employee_month.entity_id = '$P!{employee_id}')
AND (vw_employee_month.period_year = '$P!{period_year}')
$P!{organd}

ORDER BY vw_employee_month.start_date]]>
	</queryString>
